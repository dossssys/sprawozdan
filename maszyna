title(['9', Nazwa(DZIEW)]);

for k=1:1:20 %tutaj musi byc liczba wszystkich ww macierzy
    for z=1:1:4
        cos = WZZ(:,z);
        for n=1:1:18 %bo mamy 50 procent zaszumienia
                     %a 50 procent z 36 to 18
            ind = randperm(36); %bo mamy 6*6 znakow=36
                                %randperm - losowanie liczb
            cos(ind(1))= ~cos(ind(1));
            for i=1:1:4 %pierdol to
                g(i) = 2*WZZ(:,i)'*cos - WZZ(:,i)'*WZZ(:,i); %patrz wzor
            end
            [maxx, ind] = max(g)
            cos = WZZ(:,z);
        end
    end
end
