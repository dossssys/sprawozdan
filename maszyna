WZZ =[A AA B C CC D E EE F G H I J K L LL M N NN O OO P R S SS T U W Y Z ZZ ZZZ];

Nazwa =
['A';'?';'B';'C';'?';'D';'E';'?';'F';'G';'H';'I';'J';'K';'L';'?';'M';'N';'?
';'O';'?';'P';'R';'S';'?';'T';'U';'W';'Y';'Z';'?';'?'];
